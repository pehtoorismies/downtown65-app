{
  "name": "downtown65-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "sst start",
    "build": "lerna run build --scope @downtown65-app/common && sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "production:build": "lerna run build --scope @downtown65-app/common && sst build --stage production",
    "production:deploy": "sst deploy --stage production",
    "typecheck": "lerna run typecheck --no-sort",
    "test": "lerna run test",
    "lint": "lerna run lint --no-sort",
    "find-deadcode": "yarn workspaces run find-deadcode",
    "generate": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-graphql-request": "4.5.8",
    "@graphql-codegen/typescript-operations": "3.0.2",
    "@tsconfig/node16": "16.1.0",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "aws-cdk-lib": "2.84.0",
    "constructs": "10.2.69",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "lerna": "6.5.1",
    "prettier": "3.0.1",
    "prettier-eslint": "15.0.1",
    "sst": "2.23.15",
    "ts-node": "10.9.1",
    "ts-prune": "^0.10.3",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "4.0.7",
    "vitest": "0.29.7"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "date-fns": "2.29.3",
    "pino": "8.11.0",
    "pino-lambda": "4.1.0",
    "pino-pretty": "10.0.0"
  }
