{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "sst bind remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc --noEmit",
