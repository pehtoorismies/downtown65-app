    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "cypress": "12.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-unused-imports": "2.0.0"
  },
  "engines": {
    "node": ">=16"
